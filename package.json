{
	"name": "react-app-boilerplate",
	"version": "1.0.0",
	"description": "A simple react application boilerplate",
	"main": "index.js",
	"scripts": {
		"dev": "cross-env NODE_ENV=development  webpack-dev-server --config webpack/config.dev.js",
		"build:prod": "cross-env NODE_ENV=prod webpack --config webpack/config.prod.js",
		"build:qa": "cross-env NODE_ENV=qa webpack --config webpack/config.prod.js",
		"build:uat": "cross-env NODE_ENV=uat webpack --config webpack/config.prod.js",
		"build:test": "cross-env NODE_ENV=test webpack --config webpack/config.prod.js",
		"build:icon": "node ./webpack/config.iconfont.js",
		"preview:icon": "npm run build:icon && http-server ./static/fonts/",
		"mock": "node server/app.js",
		"lint": "eslint --fix --ext .jsx,js .",
		"cspell": "cspell 'src/**/*.{jsx,js,scss}'",
		"prettier": "prettier --write 'src/**/*.{jsx,js}'",
		"stylelint": "stylelint **/*.{css,scss,less,styl} --fix",
		"test": "jest --passWithNoTests",
		"test:coverage": "jest --coverage"
	},
	"homepage": "http://qinghuanI.github.io/react-app-boilerplate",
	"keywords": [
		"react",
		"webpack"
	],
	"author": "qinghuanI",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/eslint-parser": "^7.12.1",
		"@babel/plugin-transform-react-jsx": "^7.12.7",
		"@babel/plugin-transform-runtime": "^7.12.1",
		"@babel/preset-env": "^7.12.7",
		"@babel/preset-react": "^7.12.7",
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@hot-loader/react-dom": "^16.8.6",
		"autoprefixer": "^10.0.4",
		"babel-loader": "^8.2.2",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^6.1.1",
		"cross-env": "^7.0.2",
		"cspell": "^4.2.2",
		"css-loader": "^5.0.1",
		"cssnano": "^4.1.10",
		"eslint": "^7.14.0",
		"eslint-config-standard": "^16.0.2",
		"eslint-plugin-html": "^6.1.1",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.1.3",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-webpack-plugin": "^2.4.1",
		"extract-loader": "^5.1.0",
		"favicons-webpack-plugin": "^4.2.0",
		"file-loader": "^6.2.0",
		"hard-source-webpack-plugin": "^0.13.1",
		"html-webpack-plugin": "^4.5.0",
		"http-server": "^0.12.3",
		"husky": "^4.3.0",
		"icon-font-loader": "^3.0.2",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.2",
		"mini-css-extract-plugin": "^0.11.0",
		"node-sass": "^5.0.0",
		"portfinder": "^1.0.28",
		"postcss": "^8.1.10",
		"postcss-loader": "^4.1.0",
		"prettier": "^2.2.1",
		"react-dev-inspector": "^1.1.0",
		"react-hot-loader": "^4.13.0",
		"react-refetch": "^3.0.1",
		"sass": "^1.29.0",
		"sass-loader": "^10.1.0",
		"style-loader": "^2.0.0",
		"stylelint": "^13.8.0",
		"stylelint-config-standard": "^20.0.0",
		"stylelint-order": "^4.1.0",
		"stylelint-webpack-plugin": "^2.1.1",
		"svgtofont": "^3.8.2",
		"terser-webpack-plugin": "^3.0.3",
		"url-loader": "^4.1.1",
		"webpack": "^4.44.2",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^5.4.0",
		"webpackbar": "^5.0.0-3"
	},
	"dependencies": {
		"@loadable/component": "^5.14.1",
		"axios": "^0.21.0",
		"dayjs": "^1.9.7",
		"i18next": "^19.8.4",
		"js-cookie": "^2.2.1",
		"lodash": "^4.17.20",
		"prop-types": "^15.7.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-i18next": "^11.8.5",
		"react-router-config": "^5.1.1",
		"react-router-dom": "^5.2.0",
		"react-toastify": "^7.0.3",
		"recoil": "^0.1.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
		}
	},
	"sideEffects": [
		"*.css",
		"*.scss"
	]
}
