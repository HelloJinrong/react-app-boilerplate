{
	"name": "react-app-boilerplate",
	"version": "1.0.0",
	"description": "A simple react application boilerplate",
	"main": "index.js",
	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack/config.dev.js",
		"build": "cross-env NODE_ENV=production webpack --config webpack/config.prod.js",
		"build:dll": "webpack --config webpack/config.dll.js --mode development",
		"mock": "node server/app.js",
		"lint": "eslint --fix --ext .jsx,js .",
		"cspell": "cspell 'src/**/*.{jsx,js,scss}'",
		"prettier": "prettier --write 'src/**/*.{jsx,js}'",
		"stylelint": "stylelint **/*.{css,scss,less,styl} --fix",
		"test": "jest --passWithNoTests"
	},
	"keywords": [
		"react",
		"webpack"
	],
	"author": "qinghuanI",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/eslint-parser": "^7.12.1",
		"@babel/plugin-transform-react-jsx": "^7.12.7",
		"@babel/plugin-transform-runtime": "^7.12.1",
		"@babel/preset-env": "^7.12.7",
		"@babel/preset-react": "^7.12.7",
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@hot-loader/react-dom": "^16.8.6",
		"add-asset-html-webpack-plugin": "^3.1.3",
		"autoprefixer": "^10.0.4",
		"babel-loader": "^8.2.2",
		"clean-webpack-plugin": "^3.0.0",
		"cross-env": "^7.0.2",
		"cspell": "^4.2.2",
		"css-loader": "^5.0.1",
		"cssnano": "^4.1.10",
		"eslint": "^7.14.0",
		"eslint-config-standard": "^16.0.2",
		"eslint-plugin-html": "^6.1.1",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.1.3",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.21.5",
		"eslint-webpack-plugin": "^2.4.1",
		"favicons-webpack-plugin": "^4.2.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^4.5.0",
		"husky": "^4.3.0",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.2",
		"mini-css-extract-plugin": "^1.3.1",
		"portfinder": "^1.0.28",
		"postcss": "^8.1.10",
		"postcss-loader": "^4.1.0",
		"prettier": "^2.2.1",
		"react-dev-inspector": "^1.1.0",
		"react-hot-loader": "^4.13.0",
		"sass": "^1.29.0",
		"sass-loader": "^10.1.0",
		"style-loader": "^2.0.0",
		"stylelint": "^13.8.0",
		"stylelint-config-standard": "^20.0.0",
		"stylelint-order": "^4.1.0",
		"terser-webpack-plugin": "^3.0.3",
		"url-loader": "^4.1.1",
		"webpack": "^4.44.2",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^5.4.0",
		"webpackbar": "^5.0.0-3"
	},
	"dependencies": {
		"@loadable/component": "^5.14.1",
		"axios": "^0.21.0",
		"dayjs": "^1.9.7",
		"formik": "^2.2.5",
		"koa": "^2.13.0",
		"koa-bodyparser": "^4.3.0",
		"koa-cors": "0.0.16",
		"koa-router": "^10.0.0",
		"lodash": "^4.17.20",
		"prop-types": "^15.7.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router-config": "^5.1.1",
		"react-router-dom": "^5.2.0",
		"recoil": "^0.1.2",
		"swr": "^0.3.9",
		"yup": "^0.32.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
		}
	},
	"sideEffects": [
		"./src/**/*.scss",
		"./src/**/*.css"
	]
}
